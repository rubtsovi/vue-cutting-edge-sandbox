{
  "name": "toggl-invoice-maker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:https": "cross-env IS_SECURE=true vite",
    "build": "vue-tsc && vite build",
    "build:dev": "vue-tsc && vite build --mode staging",
    "preview": "vite preview",
    "lint": "eslint . --config eslint.config.cjs --ext vue,ts --report-unused-disable-directives --max-warnings 0",
    "format": "eslint --config eslint.config.cjs src --ext vue,ts --cache --fix && prettier --write \"./**/*.{ts,vue,json}\""
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.11.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.3",
    "vue-eslint-parser": "^9.4.0",
    "vue-tsc": "^1.8.25"
  }
}
